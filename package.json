{
  "name": "eggnet-monitor",
  "version": "1.0.0",
  "description": "Monitoring of the Chest Mimic familiar's DNA Lab contents in the online RPG, Kingdom of Loathing",
  "main": "src/server.ts",
  "scripts": {
    "start": "node --env-file=.env --import tsx src/server.ts",
    "dev": "tsx watch --env-file=.env src/server.ts",
    "update": "node --env-file=.env --import tsx src/update.ts",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "express": "^5.1.0",
    "jsdom": "^26.1.0",
    "kol.js": "^0.1.8",
    "tsx": "^4.19.4"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.0.0",
    "prettier": "^3.5.3",
    "prisma": "^6.9.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "GPL-3.0-or-later",
  "author": "Semenar <semenuska201@yandex.ru>",
  "packageManager": "yarn@4.9.2"
}
